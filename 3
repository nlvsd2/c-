#include <stdio.h>

int main()
{
    unsigned char num1 = 10; //부호 X unsigned

    unsigned short num2 = 12492;

    signed int num3 = -19283948; //일반 정수일 경우(부호 상관 X) ==> %d 사용할 것!! 아니면 숫자 이상해

    unsigned long num4 = 12849894;

    unsigned long long num5 = 1918439138983;

    printf("%u %u %d %lu %llu\n", num1, num2, num3, num4, num5);
    
    return 0;
}
